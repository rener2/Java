<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <LineChart fx:id="lineChart" layoutY="50.0" legendSide="RIGHT" title="Chart">
        <xAxis>
          <CategoryAxis label="x" side="RIGHT" />
        </xAxis>
        <yAxis>
          <NumberAxis label="y" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="149.0" layoutY="450.0" mnemonicParsing="false" onAction="#xController" text="Like Chart" />
      <TextField fx:id="inputField" layoutX="109.0" layoutY="425.0" promptText="4 x 3 + 3 x v- 3 x + 1" text="4x^3 + 3x^2 - 3x + 1" />
   </children>
</AnchorPane>
